{
  "$id": "https://example.com/arrays.schema.json",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "description": "A schema with an array",
  "type": "object",
  "properties": {
    "fruits": {
      "type": "array",
      "examples": [
        [
          "apple",
          "banana"
        ]
      ],
      "items": {
        "type": "string",
        "examples": ["apple"]
      }
    }
  }
}
